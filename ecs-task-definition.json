{
  "family": "proof-of-play-transactions",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "proof-of-play",
      "image": "127430557055.dkr.ecr.ap-southeast-2.amazonaws.com/proof-of-play:transactions-latest",
      "essential": true,
      "memory": 16384,
      "cpu": 4096,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/proof-of-play",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "4096",
  "memory": "16384",
  "executionRoleArn": "arn:aws:iam::127430557055:role/ecsTaskExecutionRole"
}
