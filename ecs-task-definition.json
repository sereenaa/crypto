{
  "family": "arb-opcodes",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "arb-opcodes",
      "image": "796973488332.dkr.ecr.ap-southeast-2.amazonaws.com/arb-opcodes:arb-opcodes-latest",
      "essential": true,
      "memory": 8192,
      "cpu": 1024,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/arb-opcodes",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "executionRoleArn": "arn:aws:iam::796973488332:role/ecsTaskExecutionRole"
}